# Input file

start parameters
    nItr = 10000000 #iterations
    timeStep = 0.1 #in us


    timeWrite = 100
    trajWrite = 1000
    restartWrite = 1000
    fromRestart = false

    overlapSepLimit = 7.0
end parameters

start boundaries
    WaterBox = [500,500,500] #nm.
end boundaries

start molecules
    clat
end molecules

# start observables
#     clat
# end observables

start reactions
    ### CLAT - CLAT ###
    clat(cd1) + clat(cd2) <-> clat(cd1!1).clat(cd2!1)
    onRate = 100
    offRate = 10
    norm1 = [0,0,1]
    norm2 = [0,0,1]
    sigma = 1.0
    assocAngles = [M_PI,M_PI,nan,nan,0]
    observeLabel = leg
    bindRadSameCom = 5.0

    clat(cd1) + clat(cd3) <-> clat(cd1!1).clat(cd3!1)
    onRate = 100
    offRate = 10
    norm1 = [0,0,1]
    norm2 = [0,0,1]
    sigma = 1.0
    assocAngles = [M_PI,M_PI,nan,nan,0]
    observeLabel = leg
    bindRadSameCom = 5.0

    clat(cd2) + clat(cd3) <-> clat(cd2!1).clat(cd3!1)
    onRate = 100
    offRate = 10
    norm1 = [0,0,1]
    norm2 = [0,0,1]
    sigma = 1.0
    assocAngles = [M_PI,M_PI,nan,nan,0]
    observeLabel = leg
    bindRadSameCom = 5.0

    clat(cd1) + clat(cd1) <-> clat(cd1!1).clat(cd1!1)
